<h1>All Social Workers</h1>

<ul class="list-group">
<% @social_workers.each do |sw| %>
  <li class="list-group-item d-flex justify-content-between align-items-center">
    <%= link_to sw.name, social_worker_path(sw) %>
    <span class="badge badge-primary badge-pill"><%=sw.num_cases%></span>
  </li>
  <% end %>
</ul>